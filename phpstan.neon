includes:
    - ./vendor/nunomaduro/larastan/extension.neon
    - ./vendor/yamadashy/phpstan-formatter-friendly/extension.neon

parameters:

    paths:
        - app

    # The level 9 is the highest level
    level: 5

    ignoreErrors:
        - '#PHPDoc tag @var#'

    excludePaths:
        - ./*/*/FileToBeExcluded.php

    checkMissingIterableValueType: false
